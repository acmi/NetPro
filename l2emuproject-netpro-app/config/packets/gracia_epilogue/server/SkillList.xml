		</dword>
		<byte alias="Disabled" type="YesOrNo">
			<scriptAlias id="LUA_OWNED_SKILL_DISABLED" />
		</byte>
		<byte alias="Enchantable" type="YesOrNo" /> <!-- NEW -->
	</loop>
